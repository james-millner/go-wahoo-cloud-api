{
  "request": {
    "method": "POST",
    "urlPattern": "/oauth/token"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "access_token": "YOUR_ACCESS_TOKEN",
      "token_type": "bearer",
      "expires_in": 3600,
      "refresh_token": "YOUR_REFRESH_TOKEN"
    }
  }
}